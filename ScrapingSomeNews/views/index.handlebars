<div>
    <h1 class="card z-depth-3 center-align">Read the Latest from 'MLB.com'</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' to get the latest articles</h5>
    </div>
</div>
<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each story}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readStory/{{this.id}}" class="waves-effect waves-light btn" id="btnSignUp">Read Story</a>
        </div>

        {{/each}}
    </div>
</div>